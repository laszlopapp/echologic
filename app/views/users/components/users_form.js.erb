/**
 * Template to view user registration/signin/basic profile modal dialog.
 */

/* Write render output into JS variables for later use. */
var content = "<%= escape_javascript(render(:partial => partial)) %>";

$('#dialog_content').html(content);

$('#dialog_content').dialog({
  bgiframe: true,
  modal:    true,
  close:    function() { $(this).dialog('destroy'); },
  buttons: {},
  width:    820,
  height:   540,
  resizable: false,
  dialogClass: 'signinup_dialog'
});
